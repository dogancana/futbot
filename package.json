{
  "name": "futbot",
  "version": "1.2.1",
  "description": "Small size tradebot for automating simple tasks",
  "main": "lib/app.ts",
  "scripts": {
    "start": "ts-node --inspect=5858 lib/app.ts",
    "dev": "nodemon",
    "docs": "typedoc --out ./doc lib",
    "lint": "tslint 'lib/**/*.ts'",
    "build": "webpack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.18.1",
    "body-parser": "1.18.3",
    "cheerio": "1.0.0-rc.2",
    "cors": "2.8.4",
    "cors-anywhere": "0.4.1",
    "dotenv": "6.2.0",
    "express": "4.16.3",
    "lodash": "4.17.11",
    "object-hash": "1.3.0",
    "rxjs": "6.3.3",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/lodash": "4.14.144",
    "@types/node": "10.10.1",
    "husky": ">=1",
    "lint-staged": ">=8",
    "nodemon": "1.18.9",
    "pkg": "4.4.0",
    "prettier": "1.18.2",
    "ts-loader": "6.2.0",
    "ts-node": "7.0.1",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tsoa": "2.2.5",
    "typescript": "3.0.3",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{ts,js,vue,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
